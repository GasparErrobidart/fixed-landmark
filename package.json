{
  "name": "fixed-landmark",
  "version": "1.3.0",
  "description": "A library for fixing elements to a container within a container.",
  "main": "index.js",
  "scripts": {
    "distribution": "npm run concat && npm run babelify && npm run minify",
    "concat" : "node concat --files \"src/box.js,src/container.js,src/landmark.js,src/fixed-element.js\" --output \"fixed-landmark.js\"",
    "babelify": "babel bundle -d dist ",
    "minify": "node minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GasparErrobidart/fixed-landmark.git"
  },
  "keywords": [
    "fixed",
    "element",
    "js",
    "browser",
    "scrollspy",
    "scroll"
  ],
  "author": "Gaspar G. Errobidart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GasparErrobidart/fixed-landmark/issues"
  },
  "devDependencies": {
    "braces": ">=2.3.1",
    "babel-cli": "^6.0.0",
    "babel-preset-env": "^1.7.0",
    "minify": "^4.1.3",
    "minimist": "^1.2.0"
  },
  "homepage": "https://github.com/GasparErrobidart/fixed-landmark#readme"
}
